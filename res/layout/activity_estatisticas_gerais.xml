<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal|center_vertical"
    tools:context=".activity.estatisticas_gerais">
    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="EstatÃ­sticas"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@android:color/white"
        android:textSize="40dp"
        android:textStyle="normal|bold|italic" />
    <TextView
        android:id="@+id/textView23"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_marginTop="25dp"
        android:background="#ffffff"
        android:text="NOME"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#177c25"
        android:textSize="@dimen/botaoIniciar"
        android:textStyle="normal|bold|italic" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="58dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_marginStart="7dp"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="25dp"
        android:layout_toEndOf="@+id/textView23"
        android:layout_toRightOf="@+id/textView23"
        android:background="#ffffff"
        android:text="GOLS"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#177c25"
        android:textSize="@dimen/botaoIniciar"
        android:textStyle="normal|bold|italic" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="45dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_marginStart="7dp"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="25dp"
        android:layout_toEndOf="@+id/textView18"
        android:layout_toRightOf="@+id/textView18"
        android:background="#ffffff"
        android:text="ASS"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#177c25"
        android:textSize="@dimen/botaoIniciar"
        android:textStyle="normal|bold|italic" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="45dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="25dp"
        android:layout_toEndOf="@+id/textView19"
        android:layout_toRightOf="@+id/textView19"
        android:background="#ffffff"
        android:text="CA"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#177c25"
        android:textIsSelectable="false"
        android:textSize="@dimen/botaoIniciar"
        android:textStyle="normal|bold|italic" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_toEndOf="@+id/textView20"
        android:layout_toRightOf="@+id/textView20"
        android:background="#ffffff"
        android:text="CV"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#177c25"
        android:textIsSelectable="false"
        android:textSize="@dimen/botaoIniciar"
        android:textStyle="normal|bold|italic" />

    <ListView
        android:id="@+id/LVAllId"
        android:layout_width="fill_parent"
        android:layout_height="400dp"
        android:layout_below="@+id/textView23"
        android:layout_marginTop="5dp"
        android:divider="@color/colorAccent"
        android:dividerHeight="1dp" />

    <ImageView
        android:id="@+id/BotaoVoltarEstatisticasGeraisId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LVAllId"
        android:layout_marginLeft="180dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ic_exit_to_app"
        android:onClick="voltarParaTelaPrincipal" />

</RelativeLayout>