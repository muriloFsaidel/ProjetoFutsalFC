<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.DadosJogadorUsuario">

    <TextView
        android:id="@+id/textViewJogadorUsuario"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="415dp"
        android:gravity="clip_vertical|center_horizontal"
        android:text="Bem-vindo ao FutsalFC"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="@dimen/botaoIniciar"
        android:textStyle="bold|italic" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="fill_parent"
        android:layout_height="168dp"
        android:layout_marginTop="450dp"
        android:background="@drawable/ligafutsal" />

    <Button
        android:id="@+id/btVisualizarJogador"
        android:layout_width="160dp"
        android:layout_height="130dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="70dp"
        android:background="@color/cor_botao_darker"
        android:padding="10dp"
        android:text="Visualizar Jogador"
        android:textSize="20dp"
        android:textStyle="bold|italic" />

    <Button
        android:id="@+id/btConfirmarJogadorUsuario"
        android:layout_width="160dp"
        android:layout_height="130dp"
        android:layout_marginLeft="210dp"
        android:layout_marginTop="70dp"
        android:background="@color/cor_botao_darker"
        android:padding="10dp"
        android:text="Atualizar dados Jogador UsuÃ¡rio"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/TvVerificarEmailDadosJogadorUsuarioId"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:gravity="center_horizontal" />


</FrameLayout>