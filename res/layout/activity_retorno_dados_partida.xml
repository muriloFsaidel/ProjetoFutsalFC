<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal|center_vertical"
    tools:context=".activity.RetornoDadosPartida">

    <ListView
        android:id="@+id/LVRetornoId"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_below="@+id/textView38"
        android:layout_marginTop="25dp"
        android:divider="@color/colorAccent"
        android:dividerHeight="1dp" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TVPartidaId"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="26dp"
        android:text="J  |  G  |  A  |  CA  |  CV"
        android:textColor="@android:color/background_light"
        android:textSize="@dimen/botaoIniciar"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/TVPartidaId"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="TextView"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        android:textStyle="bold|italic" />

    <ImageView
        android:id="@+id/bVoltarId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LVRetornoId"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/ic_exit_to_app"
        android:foregroundGravity="center_horizontal"
        android:onClick="voltar5" />


</RelativeLayout>